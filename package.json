{
  "name": "venusprotocol",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=20.x.x"
  },
  "workspaces": ["packages/*", "configs/*", "apps/*"],
  "scripts": {
    "start": "turbo run start --",
    "generate": "turbo run generate --",
    "lint": "turbo run lint --",
    "lint:styles": "turbo run lint:styles --",
    "format": "biome check --apply .",
    "test": "turbo run test --",
    "tsc": "turbo run tsc --",
    "clean": "turbo run clean && rm -rf node_modules",
    "build": "turbo build --",
    "extract-translations": "turbo extract-translations",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "postinstall": "husky install && yarn run generate"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/config-conventional": "17.7.0",
    "commitlint": "^18.1.0",
    "husky": "^9.0.6",
    "reg-keygen-git-hash-plugin": "^0.14.2",
    "reg-notify-github-plugin": "^0.14.2",
    "reg-publish-s3-plugin": "^0.12.2",
    "reg-suit": "^0.14.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.68.0",
    "turbo": "^1.11.3",
    "typescript": "^5.1.6",
    "validate-branch-name": "^1.3.0"
  },
  "resolutions": {
    "react-error-overlay": "6.0.11",
    "**/@emotion/styled": "^11.0.0",
    "chrome-remote-interface": "^0.32.1"
  }
}
