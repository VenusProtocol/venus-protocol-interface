{
  "name": "venusprotocol",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18.x.x"
  },
  "workspaces": [
    "packages/*",
    "configs/*",
    "apps/*"
  ],
  "scripts": {
    "start": "turbo run start",
    "generate": "turbo run generate",
    "lint": "turbo run lint",
    "lint:ts": "turbo run lint:ts",
    "lint:styles": "turbo run lint:styles",
    "lint:format": "yarn prettier **/*.{js,ts,tsx,html,json}",
    "format": "yarn lint:format --write && turbo format",
    "test": "turbo run test",
    "tsc": "turbo run tsc",
    "clean": "turbo run clean && rm -rf node_modules",
    "build": "turbo build",
    "extract-translations": "turbo extract-translations",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "postinstall": "husky install && yarn run generate"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/config-conventional": "17.7.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "commitlint": "^18.1.0",
    "husky": "^9.0.6",
    "lint-staged": "^15.0.1",
    "prettier": "^3.0.3",
    "prettier-airbnb-config": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "reg-keygen-git-hash-plugin": "^0.13.0",
    "reg-notify-github-plugin": "^0.14.2",
    "reg-publish-s3-plugin": "^0.12.2",
    "reg-suit": "^0.14.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.68.0",
    "turbo": "^1.11.3",
    "typescript": "^5.1.6",
    "validate-branch-name": "^1.3.0"
  },
  "resolutions": {
    "react-error-overlay": "6.0.11",
    "**/@emotion/styled": "^11.0.0",
    "chrome-remote-interface": "^0.32.1"
  }
}
