{
  "name": "venus",
  "version": "2.2.3",
  "private": true,
  "engines": {
    "node": ">=18.x.x"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "TZ=UTC VITE_ENVIRONMENT=testnet vitest run",
    "lint:format": "yarn prettier \"src/**/*.{js,jsx,ts,tsx,html,json,md}\" public/**/*.html --cache --check",
    "lint": "npm-run-all --parallel lint:ts lint:styles",
    "lint:ts": "export NODE_OPTIONS=\"--max-old-space-size=4096\" && eslint . --cache",
    "lint:styles": "stylelint 'src/**/*.{css,scss,ts,tsx,js,jsx}'",
    "typecheck": "tsc",
    "format": "yarn lint:format --write && yarn lint:ts --fix && yarn lint:styles --fix",
    "extract-translation-keys": "i18next",
    "generate-subgraph-types:mainnet": "yarn graphql-codegen --config ./src/config/codegen.ts --project mainnet",
    "generate-subgraph-types:testnet": "yarn graphql-codegen --config ./src/config/codegen.ts --project testnet",
    "generate-subgraph-types": "npm-run-all --parallel generate-subgraph-types:mainnet generate-subgraph-types:testnet",
    "generate-contract-types": "rimraf src/types/contracts && typechain --target=ethers-v5 --out-dir=src/types/contracts 'src/constants/contracts/abis/*.json'",
    "generate-pancake-swap-tokens": "node ./scripts/generatePancakeSwapTokens.mjs && yarn prettier --write ./src/constants/tokens/swap/mainnetPancakeSwapTokens.ts",
    "generate-version-file": "genversion --es6 --semi src/constants/version.ts",
    "postinstall": "npm-run-all --parallel generate-contract-types generate-subgraph-types generate-pancake-swap-tokens generate-version-file",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "regression": "reg-suit run"
  },
