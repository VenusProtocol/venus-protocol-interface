{
  "name": "venus",
  "version": "0.3.0",
  "private": true,
  "engines": {
    "node": ">=16.x.x"
  },
  "scripts": {
    "start": "react-scripts --max_old_space_size=4096 start",
    "prepare": "git apply patches/@stylelint+postcss-css-in-js+0.37.2.patch",
    "build": "react-scripts --max_old_space_size=4096 build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pretty": "yarn prettier \"src/**/*.{js,jsx,ts,tsx,html,json,md}\" public/**/*.html --check",
    "lint": "export NODE_OPTIONS=\"--max-old-space-size=4096\" && eslint .",
    "lint:fix": "yarn lint --fix && yarn pretty --write && yarn lint-styles --fix",
    "lint-styles": "stylelint 'src/**/*.{css,scss,ts,tsx,js,jsx}'",
    "typecheck": "tsc",
    "extract-translation-keys": "i18next",
    "generate-contract-types": "typechain --target=web3-v1 --out-dir=./src/types/contracts './src/constants/contracts/abis/*.json'",
    "postinstall": "yarn generate-contract-types",
    "storybook": "STORYBOOK=true start-storybook -p 6006 -s public",
    "build-storybook": "STORYBOOK=true build-storybook -s public",
    "loki": "loki update --reactUri file:./storybook-static",
    "regression": "reg-suit run",
    "deploy-storybook": "storybook-to-ghpages -- --existing-output-dir=./storybook-static --source-branch=develop"
  },
  "dependencies": {
    "@binance-chain/bsc-connector": "^1.0.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.2",
    "@mui/utils": "^5.4.4",
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@types/comma-number": "^2.1.0",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/ledger-connector": "^6.1.9",
    "@web3-react/walletconnect-connector": "^6.2.13",
    "@web3-react/walletlink-connector": "^6.2.13",
    "animated-number-react": "^0.1.1",
    "antd": "^3.26.18",
    "bignumber.js": "^9.0.0",
    "comma-number": "^2.1.0",
    "copy-to-clipboard": "^3.3.1",
    "emotion-theming": "^11.0.0",
    "ethers": "^5.0.17",
    "formik": "^2.2.9",
    "i18next": "^21.6.15",
    "lodash": "^4.17.21",
    "markdown-it": "^12.0.0",
    "moment": "^2.24.0",
    "noop-ts": "^1.0.3",
    "react": "^16.9.6",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "npm:@hot-loader/react-dom@^17.0.1",
    "react-i18next": "^11.16.5",
    "react-markdown": "^4.3.1",
    "react-markdown-editor-lite": "^1.2.2",
    "react-number-format": "^4.4.1",
    "react-query": "^3.34.16",
    "react-redux": "^7.1.0",
    "react-remarkable": "^1.1.3",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "4.0.3",
    "react-toastify": "^6.0.9",
    "react-uid": "^2.3.1",
    "recharts": "^1.8.5",
    "redux": "^4.0.4",
    "redux-actions": "^2.3.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "remarkable": "1.x",
    "sass": "^1.49.7",
    "styled-components": "^4.4.1",
    "web3": "^1.7.1",
    "web3-utils": "1.7.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.9",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@typechain/web3-v1": "^6.0.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.178",
    "@types/markdown-it": "^12.2.3",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^1.8.23",
    "@types/redux-actions": "^2.6.2",
    "@types/styled-components": "^5.1.22",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "i18next-parser": "^6.3.0",
    "lint-staged": "^12.3.3",
    "loki": "^0.25.1",
    "msw": "^0.36.8",
    "msw-storybook-addon": "^1.6.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.5.1",
    "prettier-airbnb-config": "^1.0.0",
    "react-error-overlay": "6.0.9",
    "react-powerplug": "^1.0.0",
    "reg-keygen-git-hash-plugin": "^0.11.1",
    "reg-notify-github-plugin": "^0.11.1",
    "reg-publish-s3-plugin": "^0.11.0",
    "reg-suit": "^0.11.1",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.9.2",
    "typechain": "^8.0.0",
    "typescript": "^4.5.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9",
    "babel-loader": "8.1.0",
    "**/@emotion/styled": "^11.0.0"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false,
        "fitWindow": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      },
      "chrome.ipad": {
        "target": "chrome.app",
        "width": 960,
        "height": 640,
        "deviceScaleFactor": 1,
        "mobile": true
      }
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
