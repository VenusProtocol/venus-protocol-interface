{
  "name": "venus-chains-test",
  "version": "0.8.1",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VenusProtocol/venus-protocol-interface/tree/main/packages/chains"
  },
  "files": ["build"],
  "main": "./build/index.mjs",
  "module": "./build/index.mjs",
  "types": "./build/index.d.mts",
  "scripts": {
    "start": "yarn run build -- --watch ./src",
    "build": "tsdown -d ./build",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "lint": "biome check .",
    "tsc": "tsc --noEmit",
    "generate-token-icon-manifest": "rimraf src/generated/iconSrcs.json && src/scripts/generateTokenIconManifest/index.ts",
    "generate-chain-icon-manifest": "rimraf src/generated/chainIconSrcs.json && src/scripts/generateChainIconManifest/index.ts",
    "generate": "npm-run-all --parallel generate-token-icon-manifest generate-chain-icon-manifest"
  },
  "devDependencies": {
    "@venusprotocol/isolated-pools": "^4.3.0",
    "@venusprotocol/typescript-config": "*",
    "@venusprotocol/venus-protocol": "^9.7.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsdown": "^0.15.6",
    "tsx": "^4.20.5"
  }
}
