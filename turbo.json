{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".eslint.js",
    ".nvmrc",
    ".prettierignore",
    ".prettierrc.js",
    "tsconfig.json",
    "vite.config.ts"
  ],
  "pipeline": {
    "generate": {
      "outputs": [
        "src/constants/version.ts",
        "src/libs/tokens/infos/pancakeSwapTokens/bscMainnet.ts",
        "src/libs/contracts/generated/**/*",
        "src/clients/subgraph/gql/mainnet.ts",
        "src/clients/subgraph/gql/testnet.ts"
      ]
    },
    "start": {
      "dependsOn": ["^build", "generate"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "outputs": ["coverage/**/*"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**/*"]
    },
    "lint": {},
    "lint:ts": {},
    "lint:format": {},
    "lint:styles": {},
    "tsc": {
      "cache": false
    },
    "format": {},
    "extract-translations": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
